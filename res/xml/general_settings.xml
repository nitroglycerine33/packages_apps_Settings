<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="@string/general_settings">

    <PreferenceScreen
        android:key="themes_settings"
        android:icon="@drawable/ic_settings_themes"
        android:title="@string/themes_settings_title">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.tmobile.themechooser"
            android:targetClass="com.tmobile.themechooser.ThemeChooser" />
    </PreferenceScreen>

    <PreferenceScreen
            android:key="led_settings"
            android:icon="@drawable/ic_eclipse_led"
            android:title="@string/led_settings"
            android:fragment="com.android.settings.LEDSettings" />

    <PreferenceScreen
            android:key="quiet_hours"
	    android:icon="@drawable/ic_eclipse_quiet"
            android:fragment="com.android.settings.cyanogenmod.QuietHours"
            android:title="@string/quiet_hours_title" />

    <PreferenceScreen
            android:key="about_settings"
	    android:icon="@drawable/ic_settings_about"
            android:title="@string/about_eclipse"
            android:fragment="com.android.settings.About" />

</PreferenceScreen>
